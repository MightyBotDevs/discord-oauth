{
  "name": "simple-oauth-ts",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build && npm start",
    "build": "rm -rf dist/ && tsc && resolve-tspaths",
    "start": "node --enable-source-maps .",
    "watch": "tsc-watch --onSuccess \"npm run dev\" --onFailure \"echo Compilation failed.\""
  },
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "author": "Dawid Jaworski <contact@minerpl.xyz>",
  "repository": "https://github.com/MightyBotDevs/discord-oauth",
  "private": true,
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cookies": "^0.8.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "resolve-tspaths": "^0.6.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
